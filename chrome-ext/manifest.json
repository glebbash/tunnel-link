{
  "manifest_version": 3,
  "name": "tunnel-link",
  "version": "0.0.1",
  "description": "Add a button on GitHub repo page to open/clone a repository in a vscode.dev tunnel",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["content-script.js"],
      "matches": ["https://github.com/*"]
    }
  ],
  "options_ui": { "page": "options.html" },
  "permissions": ["storage"]
}
